	<queryString>
		<![CDATA[SELECT 		 cursoopcao1.nome as curso,
		 turnocursoopcao1.nome as turno,
		 ItemProcSeletivoDataProva.dataProva,
		 sum(case when inscricao.codigo not in (select rps.inscricao from resultadoprocessoseletivo rps where rps.inscricao =  inscricao.codigo ) then 1 else 0 end) as quantidadeAusente,
		 sum(case when inscricao.codigo in (select rps.inscricao from resultadoprocessoseletivo rps where rps.inscricao =  inscricao.codigo ) then 1 else 0 end) as quantidadePresente
		 from inscricao
		 inner join pessoa as candidato on candidato.codigo = inscricao.candidato
		 inner join procseletivo on procseletivo.codigo = inscricao.procseletivo
		 inner join unidadeensinocurso as unidadeensinocursoopcao1 on unidadeensinocursoopcao1.codigo = inscricao.cursoopcao1
		 inner join curso as cursoopcao1 on cursoopcao1.codigo = unidadeensinocursoopcao1.curso
		 inner join turno as turnocursoopcao1 on turnocursoopcao1.codigo = unidadeensinocursoopcao1.turno
		 inner join ItemProcSeletivoDataProva on ItemProcSeletivoDataProva.codigo =  inscricao.itemProcessoSeletivoDataProva
		 left join SalaLocalAula as sala on sala.codigo = inscricao.sala
		 left join LocalAula as local on local.codigo = sala.LocalAula
		 where inscricao.situacao = 'CO' and procseletivo.codigo = 203
		 group by cursoopcao1.nome,  turnocursoopcao1.nome, ItemProcSeletivoDataProva.dataProva
		 order by ItemProcSeletivoDataProva.dataProva, cursoopcao1.nome, turnocursoopcao1.nome]]>
	</queryString>
