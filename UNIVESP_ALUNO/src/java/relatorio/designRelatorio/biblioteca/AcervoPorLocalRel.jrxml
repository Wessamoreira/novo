	<queryString>
		<![CDATA[SELECT
     matricula."matricula" AS matricula_matricula,
     matricula."aluno" AS matricula_aluno,
     matriculaperiodoturmadisciplina."turma" AS matriculaperiodoturmadisciplina_turma,
     matricula."curso" AS matricula_curso,
     pessoa."nome" AS pessoa_nome
FROM
     "public"."matricula" matricula INNER JOIN "public"."matriculaperiodo" matriculaperiodo ON matricula."matricula" = matriculaperiodo."matricula"
     INNER JOIN "public"."matriculaperiodoturmadisciplina" matriculaperiodoturmadisciplina ON matricula."matricula" = matriculaperiodoturmadisciplina."matricula"
     AND matriculaperiodo."codigo" = matriculaperiodoturmadisciplina."matriculaperiodo"
     INNER JOIN "public"."matriculaperiodo" matriculaperiodo_A ON matriculaperiodoturmadisciplina."matriculaperiodo" = matriculaperiodo_A."codigo"
     AND matricula."matricula" = matriculaperiodo_A."matricula"
     INNER JOIN "public"."pessoa" pessoa ON matricula."aluno" = pessoa."codigo"
WHERE
     pessoa.aluno = true]]>
	</queryString>
